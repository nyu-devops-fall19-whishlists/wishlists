[pre-commit-hook]
limit=9.0

[MESSAGES CONTROL]

disable=
    broad-except,
    cyclic-import,
    duplicate-code,
    global-statement,
    no-member,
    unnecessary-pass,
    wrong-import-position,
    logging-format-interpolation,
    too-many-lines,
	undefined-variable,
	function-redefined,
	unused-wildcard-import,
	unused-import,
	missing-function-docstring,
	unused-variable	
